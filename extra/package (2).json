{
  "name": "portfolio-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "^10.4.0",
    "axios": "^1.5.0",
    "docx": "^9.5.1",
    "framer-motion": "^10.16.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.2",
    "lucide-react": "^0.263.0",
    "mammoth": "^1.10.0",
    "pdfjs-dist": "^5.4.149",
    "postcss": "^8.4.0",
    "quill": "^2.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.5.0",
    "react-image-crop": "^11.0.10",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "node build-no-lint.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "node deploy.js",
    "preview:worker": "npm run build && wrangler dev --assets build",
    "deploy:worker": "npm run build && wrangler deploy --assets build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "wrangler": "^4.38.0"
  },
  "eslintConfig": {
    "extends": [],
    "rules": {
      "no-unused-vars": "off",
      "no-useless-escape": "off",
      "react-hooks/exhaustive-deps": "off",
      "import/no-anonymous-default-export": "off",
      "default-case": "off",
      "react/jsx-no-undef": "off"
    }
  }
}